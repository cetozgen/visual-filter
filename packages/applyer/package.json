{
  "name": "@visual-filter/applyer",
  "version": "1.0.0",
  "description": "Filter applyer utility for visual-filter",
  "author": "Obada Khalili",
  "homepage": "https://github.com/obadakhalili/vue-visual-filter.git",
  "repository": {
    "type": "git",
    "url": "https://github.com/obadakhalili/vue-visual-filter.git"
  },
  "license": "MIT",
  "files": [
    "dist"
  ],
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "dev": "yarn build --watch",
    "format": "yarn run prettier --ignore-path ../../.gitignore --write",
    "build": "tsc --project tsconfig.build.json",
    "clean": "rm -rf dist",
    "prepare": "yarn build",
    "version": "yarn format . && git add .",
    "postversion": "git push origin main && git push origin main --tags"
  },
  "dependencies": {
    "@visual-filter/common": "0.9.0"
  },
  "devDependencies": {
    "prettier": "^2.2.1",
    "typescript": "^4.2.3"
  }
}
